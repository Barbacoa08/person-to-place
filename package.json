{
  "name": "person-to-place",
  "version": "0.2.3",
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "tauri",
    "typescript",
    "timezone",
    "tracker",
    "macos",
    "windows",
    "linux"
  ],
  "scripts": {
    "all": "pnpm i && pnpm clean && pnpm format && pnpm lint && pnpm check && pnpm build && pnpm test",
    "github": "pnpm i && pnpm lint && pnpm check && pnpm build && pnpm test",
    "dev": "vite dev",
    "build": "vite build --logLevel=error",
    "clean": "rm -rf ./dist",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "pnpm prettier --write ./src* --log-level warn"
  },
  "devDependencies": {
    "@barbajoe/css-lib": "0.1.8",
    "@barbajoe/svelte-lib": "0.2.6",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/adapter-static": "2.0.3",
    "@sveltejs/kit": "1.27.7",
    "@tauri-apps/cli": "1.5.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@zerodevx/svelte-toast": "0.9.5",
    "city-timezones": "1.2.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "fuzzysort": "2.0.4",
    "prettier": "3.1.0",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "svelte-search": "2.0.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "4.5.1",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@tauri-apps/api": "1.5.1",
    "driver.js": "1.3.1",
    "iso-639-1": "3.1.0",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#v1",
    "tauri-plugin-window-state-api": "github:tauri-apps/tauri-plugin-window-state#v1"
  }
}
