{
  "name": "person-to-timezone",
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "tauri",
    "typescript",
    "timezone",
    "tracker",
    "macos",
    "windows",
    "linux"
  ],
  "scripts": {
    "all": "pnpm i && pnpm clean && pnpm format && pnpm lint && pnpm build && pnpm check && pnpm test",
    "github": "pnpm i && pnpm build && pnpm lint && pnpm check && pnpm test",
    "dev": "vite dev",
    "build": "vite build",
    "clean": "rm -rf ./dist",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "pnpm prettier --write ./src* --log-level warn"
  },
  "devDependencies": {
    "@barbajoe/css-lib": "0.1.3",
    "@barbajoe/svelte-lib": "0.2.2",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.22.3",
    "@tauri-apps/cli": "1.4.0",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@zerodevx/svelte-toast": "0.9.5",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte": "2.32.4",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "3.0.0",
    "svelte": "4.1.1",
    "svelte-check": "3.4.6",
    "svelte-search": "2.0.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6",
    "vite": "4.4.5",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@tauri-apps/api": "1.4.0",
    "@vvo/tzdb": "6.108.0",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#v1"
  }
}
